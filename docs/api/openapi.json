            "title": "Symbol",
            "type": "string"
          },
          "indicators": {
            "description": "指标名称列表",
            "items": {
              "type": "string"
            },
            "title": "Indicators",
            "type": "array"
          },
          "start_date": {
            "description": "开始日期，格式：YYYY-MM-DD",
            "title": "Start Date",
            "type": "string"
          },
          "end_date": {
            "description": "结束日期，格式：YYYY-MM-DD",
            "title": "End Date",
            "type": "string"
          },
          "use_cache": {
            "default": true,
            "description": "是否使用缓存",
            "title": "Use Cache",
            "type": "boolean"
          }
        },
        "description": "指标计算请求"
      },
      "AsyncTaskStatus": {
        "type": "object",
        "properties": {
          "task_id": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "pending",
              "processing",
              "completed",
              "failed",
              "cancelled"
            ]
          },
          "progress": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "started_at": {
            "type": "string",
            "format": "date-time"
          },
          "completed_at": {
            "type": "string",
            "format": "date-time"
          },
          "error": {
            "type": "string"
          }
        }
      },
      "IndicatorInfo": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "parameters": {
            "type": "object"
          },
          "requires_adjusted_price": {
            "type": "boolean"
          },
          "min_data_points": {
            "type": "integer"
          }
        }
      },
      "ValidationRequest": {
        "type": "object",
        "properties": {
          "symbol": {
            "description": "股票代码",
            "title": "Symbol",
            "type": "string"
          },
          "indicator": {
            "description": "指标名称",
            "title": "Indicator",
            "type": "string"
          },
          "start_date": {
            "description": "开始日期",
            "title": "Start Date",
            "type": "string"
          },
          "end_date": {
            "description": "结束日期",
            "title": "End Date",
            "type": "string"
          }
        },
        "description": "验证请求"
      },
